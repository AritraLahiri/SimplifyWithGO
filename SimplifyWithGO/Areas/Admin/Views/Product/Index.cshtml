@model List<Product>
<div class="container">
	<div class="my-5">
		<h1>Products</h1>
	</div>
	<table id="tblData" class="table table-bordered table-striped">
		<thead>
			<tr>
				<th scope="col">

				</th>
				<th scope="col">
					Name
				</th>
				<th scope="col">
					Description
				</th>	
				<th scope="col">
					Price
				</th>	
				<th scope="col">
					Category
				</th>
			</tr>
		</thead>
	</table>
	<a asp-controller="Product" asp-action="Upsert" class="btn btn-dark my-5" data-bs-toggle="tooltip" data-bs-placement="bottom" data-bs-original-title="Lets create some Category" aria-describedby="tooltip787480"> Create Product</a>

</div>

@section Scripts {
	<script src="~/js/Product.js" ></script>
	<partial name="_ValidationScriptsPartial"></partial>
}



